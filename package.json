{
  "name": "so-net",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "husky": "8.0.0",
    "lint-staged": "13.1.0",
    "prettier": "latest",
    "turbo": "1.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@8.19.3"
}
